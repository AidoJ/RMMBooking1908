{
  "name": "rejuvenators-platform",
  "version": "1.0.0",
  "description": "Unified massage booking platform with admin panel",
  "type": "module",
  "scripts": {
    "dev": "cd admin && npm run dev",
    "build": "node merge-builds.js",
    "preview": "cd admin && npm run preview",
    "admin:dev": "cd admin && npm run dev",
