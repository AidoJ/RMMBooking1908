    "test:build": "npm run build && npx http-server dist -p 3002",
    "deploy": "npm run build"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "@supabase/supabase-js": "^2.39.3",
    "stripe": "^14.25.0",
    "twilio": "^4.19.3"
  },
  "devDependencies": {
    "@types/node": "^18.16.2",
    "http-server": "^14.1.1"
  },
  "engines": {
    "node": "18.x"
  },
  "refine": {
    "projectId": "KzRnmo-KKZ8aE-7jCGlj"
  }
